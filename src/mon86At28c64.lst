   148                                  ; print_digit - print hexadecimal digit
   149                                  ; Input:
   150                                  ;	AL - bits 3...0 - digit to print (0...F)
   151                                  ; Output:
   152                                  ;	none
   153                                  ;-------------------------------------------------------------------------
   154                                  print_digit:
   155 0000018E 50                      	push	ax
   156 0000018F 53                      	push	bx
